{
  "name": "ms-ex",
  "version": "0.0.1",
  "type": "module",
  "main": "./build/server/index.js",
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf build",
    "check": "tsc --project tsconfig.json",
    "dev:local": "npm run clean && tsc-watch --onSuccess \" nodemon .\"",
    "lint": "npm run check && eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrations:up": "node ./build/migrations/up.js",
    "migrations:down": "node ./build/migrations/down.js",
    "start": "npm run migrations:up && node .",
    "test:static": "eslint . --ext .ts --max-warnings 0 && npx tsc --skipLibCheck"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.2",
    "@types/node-cron": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/swagger": "^8.9.0",
    "@fastify/swagger-ui": "^1.9.3",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "fastify": "^4.21.0",
    "jsonwebtoken": "^9.0.0",
    "nats": "^2.17.0",
    "node-cron": "^3.0.2",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "redis": "^4.6.7",
    "undici": "^5.22.1"
  }
}
